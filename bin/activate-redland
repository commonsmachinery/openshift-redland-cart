
# PATH is setup automatically via OPENSHIFT_REDLAND_PATH_ELEMENT

if [ -n "$OPENSHIFT_REDLAND_LD_LIBRARY_PATH" ]
then
    LD_LIBRARY_PATH="${LD_LIBRARY_PATH:+$LD_LIBRARY_PATH:}$OPENSHIFT_REDLAND_LD_LIBRARY_PATH"
    export LD_LIBRARY_PATH
fi        

if [ -n "$OPENSHIFT_REDLAND_PKG_CONFIG_PATH" ]
then
    PKG_CONFIG_PATH="${PKG_CONFIG_PATH:+$PKG_CONFIG_PATH:}$OPENSHIFT_REDLAND_PKG_CONFIG_PATH"
    export PKG_CONFIG_PATH
fi        

if [ -n "$OPENSHIFT_REDLAND_PYTHONPATH" ]
then
    PYTHONPATH="${PYTHONPATH:+$PYTHONPATH:}$OPENSHIFT_REDLAND_PYTHONPATH"
    export PYTHONPATH
fi        


